{
  "name": "@mastra/inngest",
  "version": "0.16.1",
  "description": "Mastra Inngest integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@inngest/realtime": "^0.4.4",
    "@opentelemetry/api": "^1.9.0",
    "inngest": "^3.44.2"
  },
  "devDependencies": {
    "inngest-cli": "1.8.2",
    "@ai-sdk/openai": "^1.3.24",
    "@hono/node-server": "^1.19.5",
    "@microsoft/api-extractor": "^7.52.8",
    "@types/node": "^20.19.0",
    "ai": "^4.3.19",
    "eslint": "^9.36.0",
    "execa": "^9.6.0",
    "get-port": "7.1.0",
    "hono": "^4.9.7",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@internal/lint": "0.0.49",
    "@mastra/core": "0.21.0",
    "@mastra/libsql": "0.15.2",
    "@mastra/deployer": "0.21.0",
    "@internal/types-builder": "0.0.24"
  },
  "peerDependencies": {
    "@mastra/core": ">=0.20.2-0 <0.22.0-0",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "workflows/inngest"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "build:watch": "pnpm build --watch",
    "test": "docker-compose up -d && vitest run --no-isolate --retry=1 && docker-compose down",
    "lint": "eslint ."
  }
}