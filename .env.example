# Core application settings
NODE_ENV=development
PORT=3000

# Database connection
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DATABASE?sslmode=require

# Microsoft Clarity
MICROSOFT_CLARITY_PROJECT_ID=your_clarity_project_id
MICROSOFT_CLARITY_API_KEY=your_clarity_api_key

# Google Analytics 4
GOOGLE_ANALYTICS_PROPERTY_ID=your_ga4_property_id

# Meta Pixel
META_PIXEL_ID=your_meta_pixel_id
META_PIXEL_ACCESS_TOKEN=your_meta_pixel_access_token

# TikTok Pixel
TIKTOK_PIXEL_ID=your_tiktok_pixel_id
TIKTOK_PIXEL_ACCESS_TOKEN=your_tiktok_pixel_access_token

# Microsoft Foundry
MICROSOFT_FOUNDRY_API_KEY=your_foundry_api_key

# Google APIs
GOOGLE_API_KEY=your_google_api_key

# Azure / other secrets
AZURE_SUBSCRIPTION_ID=your_subscription_id
AZURE_TENANT_ID=your_tenant_id
AZURE_CLIENT_ID=your_client_id
AZURE_CLIENT_SECRET=your_client_secret
